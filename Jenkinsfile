pipeline {
  agent any
  stages {
    stage('prepration') {
      steps {
        echo 'it is the preparation stage'
      }
    }
    stage('build') {
      steps {
        echo 'Project will be build'
      }
    }
    stage('Test') {
      steps {
        echo 'testing'
      }
    }
    stage('Deploye') {
      steps {
        echo 'Deploye stage'
      }
    }
  }
}